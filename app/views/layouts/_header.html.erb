<nav class="">
    <ul>
        <li> <%= link_to "Store Home", store_path %> </li>
        <% if user_signed_in? %><!-- logged in -->
          <li>Signed in as <%=current_user.email%></li>
          <li><%= link_to "Sign out", destroy_user_session_path, method: "delete" %>
        <% else %><!-- not logged in -->
          <li><%= link_to 'sign in', new_user_session_path %></li>
          <li><%= link_to 'Sign up', new_user_registration_path %></li>
        <% end %>
        <% if current_user && current_user.current_cart %>
          <li> <%= link_to "Cart", cart_path(current_user.current_cart) %></li>
        <% end %>

    </ul>
</nav>
